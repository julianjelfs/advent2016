type CellState =
    Empty
    | Wall
    | Number Char

parseCol y x c =
    case c of
        '#' -> (x, y, Wall)
        '.' -> (x, y, Empty)
        n -> (x, y, (Number n))

parseRow y =
    String.toList
        >> (List.indexedMap (parseCol y))

parseMaze =
    raw
        |> String.lines
        |> List.indexedMap parseRow
        |> List.concatMap identity


